    "build:dist": "npm run build && npm run build:func",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "dayjs": "^1.9.1",
    "firebase": "^7.23.0",
    "got": "^11.7.0",
    "v-calendar": "^1.0.8",
    "vue": "^2.6.12",
    "vue-multiselect": "^2.1.6",
    "vue-router": "^3.4.6",
    "vuefire": "^2.2.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.7",
    "@vue/cli-plugin-eslint": "^4.5.7",
    "@vue/cli-service": "^4.5.7",
    "@vue/eslint-config-standard": "^5.1.2",
    "eslint": "^7.11.0",
    "eslint-plugin-vue": "^7.0.1",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.3",
    "vue-template-compiler": "^2.6.12"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 3 versions",
    "not ie > 0"
  ],
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  }
