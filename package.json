{
  "name": "wfac-2020-registration",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:func": "netlify-lambda serve lambda",
    "build": "vue-cli-service build --modern",
    "build:func": "netlify-lambda build lambda",
    "build:dist": "npm run build && npm run build:func",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "dayjs": "^1.8.15",
    "faunadb": "^2.7.0",
    "got": "^9.6.0",
    "register-service-worker": "^1.0.0",
    "v-calendar": "^1.0.0-beta.14",
    "vue": "^2.5.16",
    "vue-multiselect": "^2.1.6",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.1",
    "@vue/cli-service": "^3.9.0",
    "@vue/eslint-config-standard": "^3.0.0-rc.10",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  }
}
